<div class="chat-header">
  <div class="left-header">
    <div class="header-title">
      <%= link_to 'イベント追加', new_event_path, class: "add-event" %>
    </div>
  </div>
  <div class="right-header">
    <div class="header-button">
      <%= link_to '全体スケジュール',  overall_schedules_path%>    
    </div>
  </div>
</div>

<div class="messages">
<hr style="border:0;border-top:1px solid red;">


<%= week_calendar events: @events do |date, events| %>
  <%= date.day %>

  <% events.each do |event| %>
    <div>
      <%= link_to "#{event.title}", "/" %>
    </div>
  <% end %>

<% end %>


<div class="other">
  <ul class="">

  </ul>
</div>